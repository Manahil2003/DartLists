main() {
  var i;
  final numbers = List<int>.filled(5, 0);
  var sum = 0;
  var avg = 0.0;

  for (i = 0; i < 5; i++) {
    stdout.write("Enter value:");
    var num = stdin.readLineSync();
    numbers[i] = int.tryParse(num!) ?? 0;
    sum = sum + numbers[i];
  }
  avg = sum / 5.0;
  print("Sum : $sum ");
  print("Average : $avg ");
}
