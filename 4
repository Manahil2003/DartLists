main() {
  var i, count = 0;
  final age = List<int>.filled(150, 0);

  stdout.write("Enter the number of person required :");
  var nInStr = stdin.readLineSync();
  var n = int.tryParse(nInStr!);

  stdout.write("Enter the ages of persons :");
  for (i = 1; i <= n; i++) {
    age[i] = int.tryParse(stdin.readLineSync()!) ?? 0;
    //var age = int.tryParse(ageInStr!);
    if (age[i] >= 50 && age[i] <= 60) {
      count = count + 1;
    }
  }
  print("$count persons are between 50 and 60");
}
